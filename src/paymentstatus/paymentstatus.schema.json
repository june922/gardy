{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "payment status",
    "description": "The payment status schema",
    "required": [
        "name"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the status."
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The status name.",
            "examples": [
                "Test"
            ]
        },
        "created_by": {
            "$id": "#/properties/created_by",
            "type": "integer",
            "title": "status created by."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the status."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the status was last updated."
        }
    }
}