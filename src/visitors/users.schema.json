{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "users",
    "description": "the users schema",
    "required": ["first_name", "last_name", "user_email", "user_password", "phone_number"],
    "additionalProperties": true,
    "properties": {
      "id": {
        "$id": "#/properties/id",
        "type": "integer",
        "title": "The database ID of the user."
      },
      "user_email": {
        "$id": "#/properties/user_email",
        "type": "string",
        "title": "The user's email. Must be unique."
      },
      "first_name": {
        "$id": "#/properties/first_name",
        "type": "string",
        "title": "The user's first name."
      },
      "last_name": {
        "$id": "#/properties/last_name",
        "type": "string",
        "title": "The user's last name."
    
      },
      "user_password": {
        "$id": "#/properties/user_password",
        "type": "string",
        "title": "The user's hashed password."
      },
      "phone_number": {
        "$id": "#/properties/phone_number",
        "type": "string",
        "title": "The user's phone number.",
        "examples": ["0700000000"]
      },
      " resident_estate": {
        "$id": "#/properties/resident estate",
        "type": "string",
        "title": "The user's resident estate."
      },
      
      "house_number": {
        "$id": "#/properties/house number",
        "type": "string",
        "title": "The user's house number."
      },
      " city_id": {
        "$id": "#/properties/city_id",
        "type": "string",
        "title": "The user's city_id."
      },
      "created_at": {
        "$id": "#/properties/created_at",
        "type": "string",
        "title": "The creation date of the user."
      },
      "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    }
  }
}
  