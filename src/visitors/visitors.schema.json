{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "users",
    "description": "the users schema",
    "required": [
      "first_name", "last_name", "national_id", "house_number", "phone_number"
    ],
    "additionalProperties": true,
    "properties": {
      "id": {
        "$id": "#/properties/id",
        "type": "integer",
        "title": "The database ID of the user."
      },
      " house_number": {
        "$id": "#/properties/tenant housenumber",
        "type": "string",
        "title": "The tenant house_number."
      },
      "first_name": {
        "$id": "#/properties/visitors_name",
        "type": "string",
        "title": "The visitors first name."
      },
      "last_name": {
        "$id": "#/properties/visitors first_name",
        "type": "string",
        "title": "The user's last name."
    
      },
      "national_id": {
        "$id": "#/properties/id number",
        "type": "string",
        "title": "The id number."
      },
      "phone_number": {
        "$id": "#/properties/phone_number",
        "type": "string",
        "title": "The user's phone number.",
        "examples": ["0700000000"]
   
      },

        "created_by": {
        "$id": "#/properties/created_by",
        "type": "string",
        "title": "The created_by."
      },
      "created_at": {
        "$id": "#/properties/created_at",
        "type": "string",
        "title": "The creation date of the user."
      },
      "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    }
  }
}
  