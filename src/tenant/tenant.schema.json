{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "tenant details",
    "description": "The tenant details schema",
    "required": [
        "tenant_name"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the tenant."
        },
        "tenant_name": {
            "$id": "#/properties/tenant name",
            "type": "string",
            "title": "The tenant name."
            
        },
        "user_id": {
            "$id": "#/properties/user_id",
            "type": "integer",
            "title": "The references the users table."
        },
        "house_id": {
            "$id": "#/properties",
            "type": "integer",
            "title": "refrences the houses tablr."
        },
        "created_by": {
            "$id": "#/properties",
            "type": "integer",
            "title": "refrences the user id of the creator."
        
        
        }
    }
}