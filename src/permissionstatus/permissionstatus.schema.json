{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "permission_status",
    "description": "The permission status schema",
    "required": [
        "status"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the status."
        },
        "status": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The status name.",
            "examples": [
                "active,pending"
            ]
        
        }
    }
}