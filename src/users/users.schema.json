{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "users",
    "description": "the users schema",
    "required": ["first_name", "last_name", "user_email", "user_password", "phone_number"],
    "additionalProperties": true,
    "properties": {
      "id": {
        "$id": "#/properties/id",
        "type": "integer",
        "title": "The database ID of the user."
      },

      "user_type_id": {
        "$id": "#/properties/id/reference the usertypes table id",
        "type": "integer",
        "title": "The database ID of the usertype."
      },
  
      "first_name": {
        "$id": "#/properties/first_name",
        "type": "string",
        "title": "The user's first name."
      },
      "last_name": {
        "$id": "#/properties/last_name",
        "type": "string",
        "title": "The user's last name."
    
      },
      
      "phone_number": {
        "$id": "#/properties/phone_number",
        "type": "string",
        "title": "The user's phone number.",
        "examples": ["0700000000"]
      },
           
      "national_id": {
        "$id": "#/properties/refrences national id/passport",
        "type": "string",
        "title": "The user'national id."
      },
      "created_by": {
        "$id": "#/properties/id/reference the user_id of the creator",
        "type": "integer",
        "title": "The database ID of the user."
      },
  
    
      "created_at": {
        "$id": "#/properties/created_at",
        "type": "string",
        "title": "The creation date of the user."
      },
      "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The date the user was last updated."
    }
  }
}
  