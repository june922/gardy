{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "vehicles",
    "description": "Thevehicles schema",
    "required": [
        "number_plate"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the vehicle."
        },
        "user_id": {
            "$id": "#/properties/owner id",
            "type": "integer",
            "title": "The database ID of the vehicle owner."
        },
        "vehicle_make": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The vehicle Make."
           
        },
        "vehicle_model": {
            "$id": "#/properties/vehicle_model",
            "type": "string",
            "title": ""
        },
        "number_plate": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The number_plate. Must be unique."
        },

        "Vehicle_color": {
            "$id": "#/properties/vehicle color",
            "type": "string",
            "title": "The color of the vehicle"
        },
        "vehicle_type": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The type.Sedan,suv,etc."    

        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the vehicle.'"
        },

        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the vehicle details were last updated."
        }
        
        
    }
}