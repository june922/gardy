{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "entry type",
    "description": "The entrytype details schema",
    "required": [
        "name"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the entrytype."
        },
        "name": {
            "$id": "#/properties/ name",
            "type": "string",
            "title": "The  name."
            
        },
        "created_by": {
            "$id": "#/properties",
            "type": "integer",
            "title": "refrences the user id of the creator."
        },
        "created_at":{
            "$id": "#/properties",
            "type":"string",
            "title":"timestamp"
        }
        
        
        }
    }
