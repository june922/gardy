{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "employees",
    "description": "The employees roles schema",
    "required": [
        "employee_id"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the employees role."
        },
        "employee_id": {
            "$id": "#/properties/refernces users table id column",
            "type": "integer",
            "title": "The ID of the employee."
        },
        "status_id": {
            "$id": "#/properties/refernces statuses table id",
            "type": "integer",
            "title": "The ID of the status"
        },
        "estate_id": {
            "$id": "#/properties/refernces estate table id",
            "type": "integer",
            "title": "The ID of the estate"
        },
        "created_by": {
            "$id": "#/properties/created_by",
            "type": "integer",
            "title": " created by."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the employees role."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the employees  were last updated."
        },
        "updated_by": {
            "$id": "#/properties/updated_by",
            "type": "integer",
            "title": "The employees who last updated the employees role."
        }
    }
}