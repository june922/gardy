{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Permissions",
    "description": "The Permissions schema",
    "required": [
        "allowed_user_id"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/references the vehicles table",
            "type": "integer",
            "title": "The database ID of the Permission ."
        },
        "vehicle_id": {
            "$id": "#/properties/vehicle _id",
            "type": "string",
            "title": "The vehicle id.",
            "examples": [
                "Test"
            ]
        },
        "user_id": {
            "$id": "#/properties/vehicle owner/tenant",
            "type": "string",
            "title": "The user id."
        },
        "allowed_user_id": {
            "$id": "#/properties/The user who is allowed to use the vehicle (e.g., guest, spouse)",
            "type": "string",
            "title": "The user id."
        },
        "permission_type_id": {
            "$id": "#/properties/The permission type id",
            "type": "string",
            "title": "The type ofpermission.Temp or perm id."        
            
        },
"expires_at": {
    "$id": "#/properties/permissiins expiry/null for permanent permissions",
    "type": "string",
    "title": "expiry time in YYYY-MM-DD format"
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of the permission."
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date the permission was last updated."
        }
    }
}