{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "tenant details",
    "description": "The tenant details schema",
    "required": [
        "start_date"
    ],
    "additionalProperties": true,
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The database ID of the additionalProperties."
        },
        "start_date": {
            "$id": "#/properties/startdate",
            "type": "string",
            "title": "The date the lease starts."
            
        },
        "end_date": {
            "$id": "#/properties/date",
            "type": "string",
            "title": "The date the lease ends."
        },
        "num_people": {
            "$id": "#/properties",
            "type": "integer",
            "title": "number of people living in the residence."
        
        },
        "pet_info": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "Pet information."
        
        }
    }
}